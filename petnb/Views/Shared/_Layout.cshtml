<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - petnb</title>

    <link rel="stylesheet" href="css/styles.css">
    <link rel="stylesheet" href="~/vendor.min.css" />

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">

</head>
<body>
    <nav id="main-navbar" class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container">
            <a class="navbar-brand" href="#">Animated Dropdown</a>
            <button id="sandwich-button" class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <ul class="nav navbar-nav">
                <li class="nav-item menu-item-custom"><a class="nav-link" href="#">About</a></li>
                <li class="nav-item"><a class="nav-link" href="#">Home</a></li>
                <li class="nav-item"><a class="nav-link" href="#">Find petsitter</a></li>
                <li class="nav-item"><a class="nav-link" href="#">Become petsitter</a></li>
                <li class="nav-item"><a class="nav-link" href="#">About</a></li>
            </ul>
            <ul class="nav navbar-nav">

                <li class="nav-item"><button class="nav-link" href="#">Login</button></li>
                <li class="nav-item"><button class="nav-link" href="#">Register</button></li>
            </ul>
        </div>
    </nav>



    @RenderBody()



    @RenderSection("Scripts", required: false)
    <script src="~/vendor.min.js"></script>
    <script src="~/js/site.js"></script>


</body>
</html>
